<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="SettingsBackgroundBrush" Color="DarkBlue"/>
            <SolidColorBrush x:Key="TabViewItemHeaderBackground" Color="DarkBlue"/>
            <SolidColorBrush x:Key="TabViewItemHeaderBackgroundSelected" Color="Blue"/>
            <SolidColorBrush x:Key="TabViewItemHeaderBackgroundPointerOver" Color="Red"/>

        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>


</ResourceDictionary>
